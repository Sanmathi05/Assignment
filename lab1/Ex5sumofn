package lab1;

import java.util.Scanner;

public class Ex5sumofn {
  
	
	
	public static void main(String[] args) {
		int sum = 0,i;
		Scanner s=new Scanner (System.in);
		System.out.println("limt of natural numbers");
		int n=s.nextInt();
		s.close();
		
		for(i=1;i<=n;i++) {
			
			if(i%3==0 && i%5==0) {
				sum+=i;
				
			}
			
			
		}
		
		System.out.println("natural no:" +sum);
		
	}

}
